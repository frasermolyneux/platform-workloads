{
    "name": "demo-manager",
    "github": {
        "description": "ClickOnce desktop application for managing Call of Duty demo recordings and downloads, published via Azure DevOps.",
        "topics": [
            "desktop",
            "click-once",
            "call-of-duty",
            "storage",
            "azure-devops-pipelines"
        ],
        "add_sonarcloud_secrets": true,
        "visibility": "public"
    },
    "environments": [
        {
            "name": "Production",
            "subscription": "sub-xi-demomanager-prd",
            "devops_project": "XtremeIdiots",
            "role_assignments": {
                "assigned_roles": [
                    {
                        "roles": [
                            "Contributor"
                        ]
                    }
                ],
                "rbac_admin_roles": [
                    {
                        "allowed_roles": [
                            "Storage Blob Data Contributor"
                        ]
                    }
                ]
            }
        }
    ]
}